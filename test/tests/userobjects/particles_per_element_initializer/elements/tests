[Tests]
  design = 'InitializerBase.md ParticlesPerElementInitializer.md'
  issues = '#36'
  requirement = "The system shall be capable of placing particles"
  [edge2]
    detail = "inside of EDGE2 elements uniformly"
    [exodus]
      input = '1d.i'
      type = 'Exodiff'
      cli_args = 'Mesh/gmg/elem_type=EDGE2
                  Outputs/file_base=edge2_out
                  Outputs/rays/file_base=edge2_rays'
      exodiff = 'edge2_out.e
                 edge2_rays.e'
      allow_test_objects = true
      detail = "and solve an electrostatic potenteial based on the particle positions"
    []
    [csv]
      input = 'edge2.i'
      type = 'CSVDiff'
      should_execute = False
      csvdiff = edge2_out.csv
      prereq = edge2/exodus
      detail = "and compute the error between an exact electrostic potential and the finite element solution"
    []
  []
